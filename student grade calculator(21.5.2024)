def calculate_grade(subject1, subject2, subject3):
    average_mark = (subject1 + subject2 + subject3) / 3
    
    if average_mark >= 90:
        return 'A'
    elif 80 <= average_mark < 90:
        return 'B'
    elif 70 <= average_mark < 80:
        return 'C'
    elif 60 <= average_mark < 70:
        return 'D'
    elif 50 <= average_mark < 60:
        return 'E'
    else:
        return 'F'

def main():
    subject1 = int(input())
    subject2 = int(input())
    subject3 = int(input())
    
    grade = calculate_grade(subject1, subject2, subject3)
    print("The grade is", grade)

if __name__ == "__main__":
    main()
